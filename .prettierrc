{
  "semi": false,
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 120,
  "endOfLine": "lf",
  "proseWrap": "preserve",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "{*.js?(on),*.yml,.*.yml,.prettierrc,*.md}",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
